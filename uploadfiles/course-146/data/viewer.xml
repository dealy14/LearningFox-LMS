		<slide id="257" showInTOC="" inSequence="" frameRate="30" totalFrames="1" swfConvertorEngine="Nova" slideDuration="15.09" advance="auto" blockMovement="false" type="normal">
			<content url="Slide1.swf" id="257"/>
